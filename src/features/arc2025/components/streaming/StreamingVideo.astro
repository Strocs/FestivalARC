---
import { cn } from '@/features/shared/utils'
import { YouTube } from '@astro-community/astro-embed-youtube'

interface Props {
  class?: string
  videoId: string
}

const { class: customClass, videoId } = Astro.props
---

<YouTube
  id={videoId}
  posterQuality='max'
  class={cn('w-full aspect-video', customClass)}
/>
